    <script>
        function download(filename, text) {
            var element = document.createElement('a');
            element.setAttribute('href', 'data:text/plain;charset=utf-8,' + encodeURIComponent(text));
            element.setAttribute('download', filename);

            element.style.display = 'none';
            document.body.appendChild(element);

            element.click();

            document.body.removeChild(element);
        }

        $("main.myme button.download-contact").click(async (e) => {
            e.preventDefault();
            $(".download-contact img, .download-contact > div").toggle();
            let params = {
                firstname: '{{data.qr_cliente_nome.split(" ")[0]}}',
                lastname: '{{data.qr_cliente_nome.split(" ")[1]}}',
                email: '{{data.qr_cliente_email}}',
                phone: '{{data.qr_cliente_telefone ? data.qr_cliente_telefone : ''}}',
                addressA: '{{data.qr_cliente_endereco_rua}} {{data.qr_cliente_endereco_numero}}',
                addressB: '{{data.qr_cliente_endereco_cidade}}',
                id: document.querySelector(`input[name="qr_id"]`).value,
